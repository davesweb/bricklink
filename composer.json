        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "friendsofphp/php-cs-fixer": "^2.10",
        "phpro/grumphp": "^0.14.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Lemmens",
            "email": "davesweb@gmail.com"
        }
    ],
    "autoload": {
